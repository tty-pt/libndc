{
  "name": "@tty-pt/ndc",
  "version": "0.3.2",
  "description": "ndc example project",
  "main": "cjs/ndc.js",
  "browser": "htdocs/ndc.js",
  "module": "esm/ndc.js",
  "entry": "ndc.js",
  "template": "index.html",
  "library": "ttyNdc",
  "exports": {
    ".": {
      "import": "./esm/ndc.js",
      "types": "./types/ndc.d.ts",
      "default": "./cjs/ndc.js"
    },
    "./ndc.css": "./htdocs/ndc.css"
  },
  "cdn": {
    "xterm": true,
    "xterm-addon-fit": true,
    "xterm-addon-web-links": true
  },
  "external": {
    "xterm": "global Terminal",
    "xterm-addon-fit": "global FitAddon",
    "xterm-addon-web-links": "global WebLinksAddon"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "scripts test",
    "build": "scripts build",
    "watch": "scripts watch",
    "lint": "scripts lint",
    "postinstall": "make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tty-pt/ndc.git"
  },
  "author": "quirinpa",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/tty-pt/ndc/issues"
  },
  "homepage": "",
  "devDependencies": {
    "@tty-pt/scripts": "^0.6.0-40"
  },
  "peerDependencies": {
    "@tty-pt/qhash": "^0.0.4",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-addon-web-links": "^0.8.0"
  },
  "keywords": [
    "ndc",
    "terminal",
    "tty",
    "WebSocket",
    "WebSockets",
    "ws",
    "session",
    "mux",
    "multiplexer"
  ],
  "serve": [
    "art"
  ],
  "bin": {
    "ndc": "./ndc-cli.js"
  },
  "files": [
    "ndc.css",
    "htdocs",
    ".index",
    "cjs",
    "esm",
    "src/*.c",
    "ndc.pc",
    "include",
    "Makefile"
  ]
}
